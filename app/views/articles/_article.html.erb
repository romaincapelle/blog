<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <% if article.photo.attached? %>
    <%= cl_image_tag article.photo.key, height: 300, width: 400, crop: :fill %>
  <% end %>

  <p>
    <strong>Body:</strong>
    <%= article.body %>
  </p>

</div>
